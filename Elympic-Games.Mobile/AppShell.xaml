<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Elympic_Games.Mobile.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Elympic_Games.Mobile.Views"
    Title="Elympic_Games.Mobile"
    FlyoutBehavior="Disabled">

    <ShellContent
        Title="Events"
        ContentTemplate="{DataTemplate local:EventsPage}"
        Route="EventsPage" />
    
    <ShellContent
        Title="Tickets Picker"
        ContentTemplate="{DataTemplate local:ScanQrCodes}"
        Route="ScanQrCodes" />

</Shell>
